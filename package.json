{
  "name": "assertron",
  "displayName": "Assertron",
  "description": "VSCode Extension to automatically generate assertions.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:assertron.generateNaiveAssert",
    "onCommand:assertron.generateFewShotAssert"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "assertron.generateNaiveAssert",
        "category": "Assertron",
        "title": "Generate Naive Assertions"
      },
      {
        "command": "assertron.generateFewShotAssert",
        "category": "Assertron",
        "title": "Generate Few Shot Assertions"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "assertron.generateNaiveAssert",
          "when": "editorFocus"
        },
        {
          "command": "assertron.generateFewShotAssert",
          "when": "editorFocus"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.6"
  }
}
